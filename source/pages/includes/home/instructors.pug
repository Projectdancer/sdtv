- const instructors = ["Adolfo & Amneris", "Adolfo indacochea", "Alain Rueda & Katerina Mik", "Carlos&Chloe", "Alex & Desiree", "Bersy Cortez", "Maykel Fonts", "Gaby & Estefy", "Gigi Bianco", "Ernesto Bulnes & Denisse Cambria", "Karen & Ricardo", "Adonis Santiago", "Gaby & Estefy", "Jessica Quiles Hernandez", "Adolfo & Amneris", "Adolfo indacochea", "Alain Rueda & Katerina Mik", "Carlos&Chloe", "Alex & Desiree", "Bersy Cortez", "Adolfo & Amneris"]

section(data-aos="fade" data-aos-duration="2000").instructors#instructors
    h2._title New classes added every month.

    ul._list
        each item, number in instructors
            li._instructor
                picture._photo
                    source(srcset=`./img/main/teachers/${number + 1}.webp`, type="image/webp")
                    img(src=`./img/main/teachers/${number + 1}.jpg` alt="Alex &amp; Desiree" width="210" height="326")
                p._name=item


    ._cta-block
        p._cta-text Learn anytime, anywhere, <br> at your own pace
        span._cta-icon
            svg(role="img" focusable="false" aria-hidden="true" width="32" height="32", fill="#B4B4C4")
                use(xlink:href="icons.svg#book")

        a(href="#find-classes")._cta-btn.button.button--border Find class for you
