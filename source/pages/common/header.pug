-
    const navLinks = {
        "Features": "#features",
        "Styles": "#styles",
        "Instructors": "#instructors",
        "Pricing": "#pricing",
        "Blog": "/blog",
        }


header.page-header
    ._wrapper.container
        a(href="/")._logo
            svg(width="152" height="25", fill="currentColor", aria-labelledby="logo-title")
                title#logo-title=title + " Logo"
                use(xlink:href="icons.svg#logo-horizontal")

        button(type="button",  aria-expanded="false", aria-controls="main-nav")._toggler
            svg(role="img", focusable="false", aria-hidden="true", width="18", height="12")._toggler-icon
                use(xlink:href="icons.svg#burger-menu")._toggler-icon-menu
                use(xlink:href="icons.svg#close", width="10", height="10", x="4", y="1")._toggler-icon-close
        nav.main-nav.-hide._nav#main-nav
            ul._list
                each val, text in navLinks
                    li._item
                        a._link(href=val)=text
            ul._list.-cta
                li
                    a.button.-white._btn(href="https://app.socialdancetv.com/login ") Log in
                li
                    a.button._btn(href="https://app.socialdancetv.com/signup/1") Sign up

